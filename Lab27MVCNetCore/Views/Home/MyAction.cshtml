@model List<string>
@{
    ViewData["Title"] = "MyAction";
}

<h2>This is my action page</h2>
<h3>Page Title is @ViewData["Title"]</h3>

<p>Some data recieved from the back end is @ViewBag.MyItem and also @ViewData["AnotherItem"]</p>
<h2>Getting data from view bag</h2>
<ul>
    @foreach (var item in ViewBag.MyList) 
    {
        <li>@item</li>
    }
</ul>

<ul>
    @for (int i = 0; i < 5; i++)
    {
        <li>Your item is @i</li>
    }
</ul>

<h2>Now try as model</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>@item</li>
    }
</ul>